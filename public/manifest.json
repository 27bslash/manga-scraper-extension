{
  "name": "Reddit Manga List",
  "description": "Scrapes manga from reddit/r/manga and common scantalation sites then adds to your reading list",
  "version": "0.39",
  "manifest_version": 3,
  "background": {
    "scripts": ["background.js"],
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "manga list"
  },
  "content_scripts": [
    {
      "matches": [
        "*://asuracomic.net/*",
        "*://asuracomic.com/*",
        "*://asuracomic.ru/*",
        "*://asuracomic.gg/*",
        "*://asuracomic.xyz/*",
        "*://asuracomics.com/*",
        "*://asuracomics.gg/*",
        "*://asuracomics.ru/*",
        "*://asuracomics.xyz/*",
        "*://asuracomics.nacm.xyz/*",
        "*://asuratoon.com/*",
        "*://asuratoon.gg/*",
        "*://asuratoon.ru/*",
        "*://asuratoon.xyz/*",
        "*://asuratoon.nacm.xyz/*",
        "*://asura.gg/*",
        "*://asura.nacm.xyz/*",
        "*://slayerscans.com/*",
        "*://mangaplus.shueisha.co.jp/*",
        "*://mangasushi.net/*",
        "*://kouhai.work/*",
        "*://realmscans.com/*",
        "*://comikey.com/*",
        "*://danke.moe/*",
        "*://reader.kireicake.com/*",
        "*://setsuscans.com/*",
        "*://luminousscans.com/*",
        "*://reaperscans.com/*",
        "*://mangadex.org/*",
        "*://dynasty-scans.com/*",
        "*://guya.moe/*",
        "*://flamescans.org/*",
        "*://viewer.heros-web.com/*",
        "*://gdstmp.site/*",
        "*://www.webtoons.com/*",
        "*://mm-scans.org/*",
        "*://leviatanscans.com/*",
        "*://lscomic.com/*",
        "*://en.leviatanscans.com/*",
        "*://onepiecechapters.com/*",
        "*://alpha-scans.org/*",
        "*://sensescans.com/*",
        "*://cosmicscans.com/*"
      ],
      "run_at": "document_end",
      "js": ["static/js/content.js"],
      "css": ["App.css"]
    }
  ],
  "permissions": ["alarms", "storage"],
  "host_permissions": [
    "*://asuracomic.net/*",
    "*://asuracomic.com/*",
    "*://asuracomic.ru/*",
    "*://asuracomic.gg/*",
    "*://asuracomic.xyz/*",
    "*://asuracomics.com/*",
    "*://asuracomics.gg/*",
    "*://asuracomics.ru/*",
    "*://asuracomics.xyz/*",
    "*://asuracomics.nacm.xyz/*",
    "*://asuratoon.com/*",
    "*://asuratoon.gg/*",
    "*://asuratoon.ru/*",
    "*://asuratoon.xyz/*",
    "*://asuratoon.nacm.xyz/*",
    "*://asura.gg/*",
    "*://asura.nacm.xyz/*",
    "*://slayerscans.com/*",
    "*://mangaplus.shueisha.co.jp/*",
    "*://mangasushi.net/*",
    "*://kouhai.work/*",
    "*://realmscans.com/*",
    "*://comikey.com/*",
    "*://danke.moe/*",
    "*://reader.kireicake.com/*",
    "*://setsuscans.com/*",
    "*://luminousscans.com/*",
    "*://reaperscans.com/*",
    "*://mangadex.org/*",
    "*://dynasty-scans.com/*",
    "*://guya.moe/*",
    "*://flamescans.org/*",
    "*://viewer.heros-web.com/*",
    "*://gdstmp.site/*",
    "*://www.webtoons.com/*",
    "*://mm-scans.org/*",
    "*://leviatanscans.com/*",
    "*://lscomic.com/*",
    "*://en.leviatanscans.com/*",
    "*://onepiecechapters.com/*",
    "*://alpha-scans.org/*",
    "*://sensescans.com/*",
    "*://cosmicscans.com/*"
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "reddit-manga-reader@example.com",
      "strict_min_version": "91.0"
    }
  }
}
